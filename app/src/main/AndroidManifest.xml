<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mursyidul.task2mvp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Task2MVP"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.produksi.UpdateActivity"></activity>
        <activity android:name=".view.produksi.InsertActivity" />
        <activity
            android:name=".view.splassSreen.SplassScreenActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.produksi.HutangActivity" />
        <activity android:name=".view.register.RegisterActivity" />
        <activity android:name=".view.login.LoginActivity" />
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>